set smartindent
set autoindent
set ignorecase
set smartcase
set regexsearch
set hlsearch
set incsearch
set imdisable

nnoremap - $
noremap [ %
noremap ] %

nnoremap j jzz
nnoremap k kzz
nnoremap n nzz
nnoremap N Nzz
nnoremap * *zz
nnoremap # #zz
nnoremap g* g*zz
nnoremap g# g#zz
nnoremap u :undo<CR>
nnoremap U :redo<CR>

vnoremap - $h

inoremap jj <Esc>
inoremap kk <Esc>

eclipseaction SearchRef  org.eclipse.jdt.ui.edit.text.java.search.references.in.project
noremap tr :SearchRef<C-m>
